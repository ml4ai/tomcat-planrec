(define
    (problem carry_stabilized)
    (:domain question_2)
    (:objects
        vic - victim
        loc - location
    )

    (:htn
        :parameters ()
        :subtasks (and 
                    (task0 (request_marker ?v ?l)) 
                    (task1 (request_carry_saved ?v ?l)))
        :ordering (and 
                    (< task0 task1))
    )

    (:init
        (and (found vic loc)
             (saved vic))
    )
)
