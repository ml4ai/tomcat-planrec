;                                (at ?v ?pos) 
;                                (retrieved ?v))
;             :effect (and (at ?p_1 ?room) 
;                          (at ?v ?room)
;                          (not (at ?p_1 ?pos))
;                          (not (at ?v ?pos)))
;        )
