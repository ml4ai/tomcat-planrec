    (define
        (problem wakeCritical-1)
        (:domain question-plan)
        (:requirements :strips :typing)

   	(:objects
        questioner - player
        answerer - player
        critical - critical
        origin - location
        dest - location
        marker - object
	    )


 55         :subtasks (and (t1 (clarify-critical-location ?qu ?c ?loc ?dest))
 56                        (t2 (request-destination ?qu ?an ?loc ?dest))
 57                        (t3 (clarify-location-repeat ?loc))
 58                        (t4 (wake-critical ?qu ?an ?c ?dest)))
 59         :ordering (and(< t1 t2)
 60                       (< t2 t4)
 61                       (< t3 t4))





	(:htn
		:parameters (?qu ?an - player ?loc ?dest - location ?c - critical)
		:subtasks (and
            (task0 (wake-critical answerer critical dest))
            (task1 (collaborate-carry-stabilized answerer critical dest marker))
		)
		:ordering (and
			(< task0 task1)
		)
	)

	(:init
        (location origin)
        (at questioner dest)
        (at critical dest)
        (at answerer origin)
	)
    (:goal
         (at answerer dest)
    )
  ); end problem definition

